CREATE DATABASE PIRATES;
SHOW DATABASES;
USE PIRATES;
CREATE TABLE PIRATE_DATA( PIRATE_NAME VARCHAR(20) NOT NULL, PIRATE_LAST_NAME VARCHAR(20) NOT NULL, WEAPON_OF_CHOICE VARCHAR(10) NOT NULL);
INSERT INTO PIRATES VALUE("ADITYA", "KUMAR", "SWORD");
SHOW * FROM PIRATES;
